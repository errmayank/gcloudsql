{
  "name": "gcloudsql",
  "version": "0.0.1",
  "description": "Minimal command line tool for adding/updating authorized networks on Google Cloud SQL",
  "main": "src/mod.ts",
  "type": "module",
  "bin": "lib/mod.js",
  "engines": {
    "node": ">=16.0.0"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "format": "prettier --write '**/*.{js,ts,json,md}'",
    "prebuild": "rimraf lib",
    "build": "rollup -c",
    "dev": "rollup -c -w"
  },
  "author": "Mayank Verma",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/errmayank/gcloudsql.git"
  },
  "bugs": {
    "url": "https://github.com/errmayank/gcloudsql/issues"
  },
  "keywords": [
    "gcloud",
    "sql",
    "google cloud platform",
    "gcloudsql",
    "cli"
  ],
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.3.3",
    "@types/inquirer": "^8.2.1",
    "@types/lodash-es": "^4.17.6",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.75.7",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-terser": "^7.0.2",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "chalk": "^5.0.1",
    "got": "^12.1.0",
    "inquirer": "^9.0.0",
    "lodash-es": "^4.17.21",
    "nanospinner": "^1.1.0"
  }
}
